def inversion(arr,n):
    
    count =0
    for i in range(n):
        for j in range(i+1,n):
            if arr[i]>arr[j]:
                count+=1
    return count            

if __name__ == "__main__":
    arr = [5,4,3,2,1]
    n = len(arr)
    count = inversion(arr,n)
    print("The number of in version is:",count)
