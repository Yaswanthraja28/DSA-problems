#passed in every test case
def containsduplicates(nums,k):
    st = set()
    for i in range(len(nums)):
        if nums[i] in st:
            return True
        st.add(nums[i])
        
        if len(st)>k:
            st.remove(nums[i-k])
    return False
            
#nums = [1,2,3,1]
nums = [1,2,3,1,2,3]
k = 2
contain = containsduplicates(nums,k)
print(contain)

passed 47 testcases out of 59
